{
  "name": "@theqrl/cli",
  "description": "QRL CLI functions",
  "version": "1.4.5",
  "author": "JP Lomas <jp@theqrl.org>, The QRL Foundation",
  "bin": {
    "qrl-cli": "./bin/run"
  },
  "bugs": "https://github.com/theqrl/qrl-cli/issues",
  "dependencies": {
    "@grpc/grpc-js": "^0.8.1",
    "@grpc/proto-loader": "^0.5.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@theqrl/qrl-proto-sha256": "^2.0.3",
    "@theqrl/validate-qrl-address": "^1.1.0",
    "aes-js": "^3.1.2",
    "aes256": "^1.0.4",
    "axios": "^0.19.0",
    "bech32": "^1.1.3",
    "bignumber.js": "^9.0.0",
    "cli-ux": "^5.3.1",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "eccrypto": "^1.1.2",
    "grpc-js-kit": "^0.1.0",
    "kleur": "^3.0.3",
    "moment": "^2.24.0",
    "node": "12.3.1",
    "ora": "^3.4.0",
    "qrcode-terminal": "^0.12.0",
    "qrllib": "^1.1.7",
    "tmp": "^0.1.0",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "codecov": "^3.6.5",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^13",
    "pkg": "^4.4.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/theqrl/qrl-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "qrl-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "theqrl/qrl-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only -t 50000 \"test/**/*.test.js\"",
    "test-ots": "nyc mocha --forbid-only -t 50000 \"test/commands/ots.test.js\"",
    "test-balance": "nyc mocha --forbid-only -t 50000 \"test/commands/balance.test.js\"",
    "test-send": "nyc mocha --forbid-only -t 50000 \"test/commands/send.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "cov": "nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov -t $CODECOV_TOKEN",
    "make-binaries": "pkg . -t node12-macos-x64 -o dist/macos/qrl-cli && pkg . -t node12-win-x64 -o dist/win/qrl-cli && pkg . -t node12-linux-x64 -o dist/linux/qrl-cli"
  }
}
